<web-app>
    <servlet>
        <servlet-name>Historico</servlet-name>
        <servlet-class>Historico</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Cartera</servlet-name>
        <servlet-class>Cartera</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Menu</servlet-name>
        <servlet-class>Menu</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>




    <servlet-mapping>
        <servlet-name>Historico</servlet-name>
        <url-pattern>/Historico</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Cartera</servlet-name>
        <url-pattern>/Cartera</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Menu</servlet-name>
        <url-pattern>/Menu</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    
</web-app>